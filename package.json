{
    "name": "daneisburgh.com",
    "author": "Dane Isburgh <hello@daneisburgh.com> (https://daneisburgh.com)",
    "repository": "https://github.com/daneisburgh/daneisburgh.com",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "prepare": "husky install",
        "commit": "cz",
        "release": "semantic-release",
        "ng": "ng",
        "start": "ng serve",
        "local": "ng serve --host=0.0.0.0 --disableHostCheck",
        "lint": "ng lint --fix",
        "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
        "e2e": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
        "build": "ng build --prod",
        "deploy": "./deploy.sh"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog",
            "maxHeaderWidth": 50
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ],
        "rules": {
            "body-max-line-length": [
                2,
                "always",
                150
            ]
        }
    },
    "lint-staged": {
        "*.ts": "eslint"
    },
    "release": {
        "branches": [
            "main"
        ],
        "npmPublish": false,
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/git",
            "@semantic-release/github"
        ]
    },
    "dependencies": {
        "@angular/animations": "^11.2.14",
        "@angular/common": "^11.2.14",
        "@angular/compiler": "^11.2.14",
        "@angular/core": "^11.2.14",
        "@angular/forms": "^11.2.14",
        "@angular/platform-browser": "^11.2.14",
        "@angular/platform-browser-dynamic": "^11.2.14",
        "@angular/router": "^11.2.14",
        "@fortawesome/angular-fontawesome": "^0.8.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/free-regular-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@popperjs/core": "^2.9.2",
        "bootstrap": "^5.0.2",
        "flat-color-icons": "^1.1.0",
        "hamburgers": "^1.1.3",
        "lodash": "^4.17.21",
        "photoswipe": "^4.1.3",
        "rxjs": "^6.6.7",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1102.14",
        "@angular-eslint/builder": "4.3.0",
        "@angular-eslint/eslint-plugin": "4.3.0",
        "@angular-eslint/eslint-plugin-template": "4.3.0",
        "@angular-eslint/schematics": "4.3.0",
        "@angular-eslint/template-parser": "4.3.0",
        "@angular/cli": "^11.2.14",
        "@angular/compiler-cli": "^11.2.14",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@types/jasmine": "^3.8.1",
        "@types/lodash": "^4.14.171",
        "@types/node": "^16.3.0",
        "@types/photoswipe": "^4.1.2",
        "@typescript-eslint/eslint-plugin": "4.20.0",
        "@typescript-eslint/parser": "4.20.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.30.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jsdoc": "35.4.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "husky": "^7.0.1",
        "jasmine-core": "~3.7.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.6.0",
        "lint-staged": "^10.5.4",
        "protractor": "~7.0.0",
        "semantic-release": "^17.4.4",
        "semantic-release-cli": "^5.4.3",
        "ts-node": "~9.1.1",
        "typescript": "~4.1.5"
    }
}
